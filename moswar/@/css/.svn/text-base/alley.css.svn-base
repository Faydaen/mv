.alley .patrol .regions-choose ul li .name b { padding:1px 4px; background:#fff; font-weight:normal; -moz-border-radius:3px; -webkit-border-radius: 3px; border-radius:3px; opacity: 0.9; text-decoration:none; cursor:default; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=90); -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)"; }
