.alley .search-alley .timeleft { font-size:11px; background:#fbd493; color:#975d17; position:absolute; bottom:0; right:0; padding:2px 4px; -moz-border-radius:5px; -webkit-border-radius: 5px; border-radius:5px; }
.alley .search-alley .collectbar { font-size:90%; }

.alley .search-name { text-align:center; padding:5px; }
