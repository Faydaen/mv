    /**
     * Генерация строки
     *
     * @param string $text
     * @param array $params
     * @return string
     */
    public static function renderText($text, $params)
    {
        foreach ($params as $code => $value) {
            $text = str_replace('<%' . $code . '%>', $value, $text);
			$text = str_replace('{%' . $code . '%}', $value, $text);
        }
        return $text;
    }
